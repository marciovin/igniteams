{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "noUnusedParameters": false,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@assets/*": [
        ".src/assets/*"
      ],
      "@components/*": [
        ".src/components/*"
      ],
      "@routes/*": [
        ".src/routes/*"
      ],
      "@screens": [
        ".src/screens/*"
      ],
      "@storage": [
        ".src/storage/*"
      ],
      "@utils/*": [
        ".src/utils/*"
      ]
    } 
  }
}
